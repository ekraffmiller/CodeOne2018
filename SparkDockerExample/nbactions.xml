<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath edu.harvard.codeone2018.simplebuild.SimpleSparkApp</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>build</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>install</goal>
            </goals>
            <properties>
                <e></e>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-dockerfile:push</actionName>
            <displayName>dockerfile:push</displayName>
            <goals>
                <goal>dockerfile:push</goal>
            </goals>
            <properties>
                <dockerfile.username>{dockerhub_accountName}</dockerfile.username>
                <dockerfile.password>{dockerhub_password}</dockerfile.password>
            </properties>
        </action>
    </actions>
